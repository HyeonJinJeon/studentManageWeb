">선생님으로 가입</b-button>\n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\n\n\nimport {firebase} from '@/firebase/firebaseConfig';\n\nexport default {\n  name: 'signUp',\n  data(){\n    return{\n      email: \"\",\n      password: \"\",\n      name: '',\n      grade: 0,\n      gender: true,\n      team: '',\n      phoneNum: '',\n    }\n  },\n  methods: {\n    studentSignup(){\n      const self = this;\n      const db = firebase.firestore();\n      firebase.auth().createUserWithEmailAndPassword(this.email, this.password)\n          .then( (result) =>{\n            db.collection('students')\n                .doc(result.user.uid)\n                .set({\n                  name: self.name,\n                  grade: self.grade,\n                  gender: self.gender,\n                  team: self.team,\n                  phoneNum: self.phoneNum,\n                })\n            alert('학생 회원가입 완료!');\n            this.$router.push('/login');\n          }).catch(err => {\n            console.error(err);\n            alert('에러 : ' + err.message)\n          })\n    },\n    teacherSignup(){\n      const self = this;\n      const db = firebase.firestore();\n      firebase.auth().createUserWithEmailAndPassword(this.email, this.password)\n          .then( (result) =>{\n            db.collection('teachers')\n                .doc(result.user.uid)\n                .set({\n                  name: self.name,\n                  gender: self.gender,\n                  team: self.team,\n                  phoneNum: self.phoneNum,\n                })\n            alert('선생님 회원가입 완료!');\n            this.$router.push('/login');\n          }).catch(err => {\n        console.error(err);\n        alert('에러 : ' + err.message)\n      })\n    },\n\n  },\n  components: {\n\n  }\n}\n</script>\n"]}]}